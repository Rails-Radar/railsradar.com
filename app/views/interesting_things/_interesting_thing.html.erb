<tr id="<%= dom_id interesting_thing %>">
  <td class="whitespace-nowrap px-2 py-4 text-sm font-medium text-gray-900"><%= interesting_thing.name %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= interesting_thing.kind %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= interesting_thing.team_id %></td>
  <td class="whitespace-nowrap px-3 py-4 text-sm text-gray-500"><%= interesting_thing.position %></td>
  <td class="relative whitespace-nowrap py-4 pl-3 pr-4 text-right text-sm font-medium sm:pr-0">
    <%= link_to "Show Thing", interesting_thing, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium bg-sky-100" %>
  </td>
  <td class="relative whitespace-nowrap py-4 pl-3 pr-4 text-right text-sm font-medium sm:pr-0">
    <%= link_to 'Edit Thing', edit_interesting_thing_path(interesting_thing), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>  </td>
</tr>
